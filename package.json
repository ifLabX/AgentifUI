{
  "name": "agentifui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:claude": "claude -p 'you are a linter. please look at the changes vs. main and report any issues related to typos. report the filename and line number on one line, and a description of the issue on the second line. do not return any other text.'",
    "format": "prettier --write .",
    "format:files": "prettier --write",
    "format:check": "prettier --check .",
    "format:staged": "lint-staged",
    "prepare": "husky",
    "type-check": "tsc --noEmit",
    "i18n:check": "python3 scripts/i18n-refactor-helper.py quick-check",
    "i18n:validate": "python3 scripts/validate-i18n-consistency.py",
    "i18n:detect": "python3 scripts/i18n-refactor-helper.py detect-missing",
    "deploy": "pnpm build && pm2 startOrRestart ecosystem.config.js --only $(node -e \"console.log(require('./ecosystem.config.js').apps[0].name)\") --update-env || { echo 'Deploy failed'; exit 1; }"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "@tailwindcss/oxide",
      "bufferutil",
      "sharp",
      "unrs-resolver",
      "utf-8-validate"
    ],
    "overrides": {
      "@eslint/plugin-kit": "^0.3.3"
    }
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-popover": "^1.1.11",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.50.0",
    "@types/prismjs": "^1.26.5",
    "@types/uuid": "^10.0.0",
    "bufferutil": "^4.0.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "fast-xml-parser": "^4.5.3",
    "framer-motion": "^11.0.8",
    "katex": "^0.16.22",
    "lodash": "^4.17.21",
    "lucide-react": "^0.513.0",
    "next": "15.3.3",
    "next-intl": "^4.3.1",
    "next-themes": "^0.4.6",
    "prism-react-renderer": "^2.4.1",
    "prismjs": "^1.30.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-easy-crop": "^5.4.2",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "rehype-katex": "^7.0.1",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "sonner": "^2.0.5",
    "tailwind-merge": "^2.2.1",
    "utf-8-validate": "^6.0.5",
    "uuid": "^11.1.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3",
    "@stagewise/toolbar-next": "^0.6.2",
    "@tailwindcss/postcss": "^4",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.15.30",
    "@types/react": "^19",
    "@types/react-dom": "^19.1.6",
    "autoprefixer": "^10.0.1",
    "eslint": "^9.31.0",
    "eslint-config-next": "15.3.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "postcss": "^8",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "tailwind-scrollbar": "^4.0.2",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md,css,scss,yaml,yml}": [
      "prettier --write"
    ]
  }
}
