# Supabase Configuration - Replace with your actual values
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here
SUPABASE_JWT_SECRET=your_jwt_secret_here

# API encryption master key
API_ENCRYPTION_KEY=your_key

# Development or testing components for production environment
ENABLE_STAGEWISE_TOOLBAR=false
ENABLE_DIFY_IFRAME=false

# SSO authentication related
SESSION_SECRET="session_secret"
NEXT_PUBLIC_APP_URL="https://your-domain"
NEXT_PUBLIC_SSO_ONLY_MODE=true

# CORS cross-origin configuration
# Production: List of domains allowed for cross-origin access, separated by commas
# Supports wildcard format: *.yourdomain.com
CORS_ALLOWED_ORIGINS=https://yourdomain.com,https://app.yourdomain.com

# Development: List of domains allowed for cross-origin access (optional)
# If not set, only allows NEXT_PUBLIC_APP_URL domain
# DEV_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001,http://127.0.0.1:3000
